{
  "expo": {
    "name": "My_Mind",
    "slug": "My_Mind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "permissions": [
      "location"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.app.my_mind",
      "supportsTablet": true,
      "config": {
        "googleServicesFile": "./GoogleService-Info.plist"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location.",
        "NSLocationAlwaysUsageDescription": "This app requires access to your location even when in the background."
      }
    },
    "android": {
      "package": "com.app.my_mind",
      "config": {
        "googleServicesFile": "./google-services.json"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
